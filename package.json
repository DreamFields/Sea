{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.9.34",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.3",
    "ajv-errors": "^1.0.1",
    "antd": "^4.6.4",
    "axios": "^0.19.2",
    "js-cookie": "^2.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.7",
    "react-highlight-words": "^0.16.0",
    "react-wavesurfer": "^0.8.6",
    "umi": "^3.2.3",
    "wavesurfer.js": "^4.0.1",
    "yorkie": "^2.0.0"
  }
}
